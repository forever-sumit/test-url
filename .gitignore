# Ignore bundler config.
/.bundle
/vendor/bundle/
/vendor/ruby/

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp/*

# configuration file introduced in Rails 4.1
/config/secrets.yml

# Ignore the database file
/config/database.yml

